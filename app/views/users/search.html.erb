<div style="padding: 20px ;display: flex; justify-content: center;" >
<h1 >SEARCH RESULT</h1>
</div>
<%#= render @users %>

<% @users.each do |user|%>
<div style="border: 1px solid; padding: 5px; margin-bottom: 10px;">
 <% if user.avatar.attached?%>
<%= image_tag user.avatar, style:"width: 100px; height: 100px"%>
 <% end%>


<b>
<%= link_to user.username ,user %><br>
<%= user.bio%></b>
<br><br>
</div>
<% end %>

